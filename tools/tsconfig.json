{
  "compilerOptions": {
    "outDir": "_never",
    "declaration": false,
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": true,
    "lib": ["dom", "esnext"],
    "target": "esnext",
    "module": "esnext",
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "include": ["omnify/bundler"]
}
